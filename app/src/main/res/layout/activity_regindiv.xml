<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".Regindiv">


  <ImageView
      android:id="@+id/logoImageView"
      android:layout_width="123dp"
      android:layout_height="78dp"
      android:contentDescription="TODO"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.975"
      app:srcCompat="@drawable/logotrans" />

  <EditText
      android:id="@+id/contactNumberEditText"
      android:layout_width="329dp"
      android:layout_height="49dp"
      android:background="@drawable/text_box2"
      android:ems="10"

      android:inputType="phone"
      android:minHeight="48dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.487"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <TextView
      android:id="@+id/registerTextView"
      android:layout_width="191dp"
      android:layout_height="49dp"
      android:text="@string/register_as"
      android:textColor="@color/lightblue"
      android:textSize="35sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.181"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.052" />

  <TextView
      android:id="@+id/textView2"
      android:layout_width="204dp"
      android:layout_height="28dp"
      android:text="Individual Donor/Company"
      android:textColor="@color/lightblue"
      android:textSize="17dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.299"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.12" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="260dp"
      android:layout_height="22dp"
      android:text="Click on your preferred account"
      android:textColor="@color/black"
      android:textSize="17dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.582"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.157" />

  <EditText
      android:id="@+id/confirmPasswordEditText"
      android:layout_width="329dp"
      android:layout_height="49dp"
      android:background="@drawable/text_box2"
      android:ems="10"
      android:inputType="textPassword"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.758" />

  <TextView
      android:id="@+id/passwordTextView"
      android:layout_width="101dp"
      android:layout_height="22dp"
      android:text="Password:"
      android:textColor="@color/lightblue"
      android:textSize="15dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.129"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.571"
      tools:ignore="TextSizeCheck" />

  <TextView
      android:id="@+id/confirmPasswordTextView"
      android:layout_width="138dp"
      android:layout_height="23dp"
      android:text="Confirm Password:"
      android:textColor="@color/lightblue"
      android:textSize="15dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.149"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.696"
      tools:ignore="TextSizeCheck" />

  <TextView
      android:id="@+id/fullNameTextView"
      android:layout_width="94dp"
      android:layout_height="20dp"
      android:text="@string/full_name"
      android:textColor="@color/lightblue"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.126"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.196"
      tools:ignore="TextSizeCheck" />

  <EditText
      android:id="@+id/emailEditText"
      android:layout_width="329dp"
      android:layout_height="49dp"
      android:background="@drawable/text_box2"
      android:ems="10"
      android:inputType="textEmailAddress"
      android:minHeight="48dp"
      android:text=" "
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.487"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.366" />

  <EditText
      android:id="@+id/fullNameEditText"
      android:layout_width="329dp"
      android:layout_height="49dp"
      android:background="@drawable/text_box2"
      android:ems="10"
      android:importantForAutofill="no"
      android:inputType="textPersonName"
      android:minHeight="48dp"
      android:text=" "
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.487"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.234" />

  <TextView
      android:id="@+id/contactNumberTextView"
      android:layout_width="130dp"
      android:layout_height="20dp"
      android:text="@string/contact_number"
      android:textColor="@color/lightblue"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.142"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.451"
      tools:ignore="TextSizeCheck" />

  <EditText
      android:id="@+id/passwordEditText"
      android:layout_width="329dp"
      android:layout_height="49dp"
      android:background="@drawable/text_box2"
      android:ems="10"
      android:inputType="textPassword"
      android:minHeight="48dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.626" />

  <TextView
      android:id="@+id/emailTextView"
      android:layout_width="79dp"
      android:layout_height="21dp"
      android:text="@string/email"
      android:textColor="@color/lightblue"
      android:textSize="15sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.123"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.322"
      tools:ignore="TextSizeCheck" />

  <Button
      android:id="@+id/registerButton"
      android:layout_width="115dp"
      android:layout_height="44dp"
      android:text="Register"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.887"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.842" />

</androidx.constraintlayout.widget.ConstraintLayout>
